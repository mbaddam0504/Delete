	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <session-config>
        <session-timeout>
            60
        </session-timeout>
    </session-config>
 <servlet>
  <servlet-name>SingleItemServlet</servlet-name>
  <servlet-class>Place.SingleItemServlet</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>SingleItemServlet</servlet-name>
  <url-pattern>/Place/*</url-pattern>
</servlet-mapping>

